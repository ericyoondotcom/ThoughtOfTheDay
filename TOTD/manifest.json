{
  "name": "Thought Of The Day New Tab",
  "description": "A new tab page that displays a random thought",
  "version": "2.0",
  "incognito": "split",
  "chrome_url_overrides": {
    "newtab": "thoughtPage.html"
  },
  "options_ui": {
    "page": "options.html"
  },
  "permissions": [
    "storage"
  ],
  "web_accessible_resources": [{
    "resources": ["resources/cog-solid.png", "resources/jquery-3.7.1.min.js"],
    "matches": ["<all_urls>"]
  }],
  "manifest_version": 3
}
